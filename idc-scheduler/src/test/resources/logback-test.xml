<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>
<configuration>
	<appender name="CONSOLE"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{5} - %msg%n
			</pattern>
		</encoder>
	</appender>

	<logger name="com.iwellmass.idc" level="DEBUG" />
	<logger name="com.iwellmass.idc.quartz.IDCJobStoreTX" level="INFO" />

	<root level="INFO">
		<appender-ref ref="CONSOLE" />
	</root>
</configuration>